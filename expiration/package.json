{
  "name": "expiration",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@mestihudson-ticketing/common": "^1.0.16",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "prebuild": "del ./build/*",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "nodemon --exec 'yarn build' --watch 'src/*' --watch 'package.json' --watch 'tsconfig.json' -e 'ts,json'",
    "lint": "eslint ./src --ext .ts",
    "lint:watch": "nodemon --exec 'yarn lint' --watch 'src/*' -e 'ts,json,js'",
    "start": "ts-node-dev -r tsconfig-paths/register --poll src/index.ts",
    "test": "jest --watchAll --no-cache --runInBand --detectOpenHandles"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "ts-jest": "^27.0.7",
    "tsc-alias": "^1.4.1"
  }
}
